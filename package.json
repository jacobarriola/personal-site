  "version": "1.0.0",
  "author": "Jacob Arriola",
  "dependencies": {
    "@reach/skip-nav": "^0.9.0",
    "gatsby": "2.19.41",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-advanced-sitemap": "^1.5.5",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-offline": "^3.2.8",
    "gatsby-plugin-postcss": "2.1.21",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-react-helmet": "3.1.23",
    "gatsby-plugin-sharp": "^2.6.10",
    "gatsby-remark-prismjs": "^3.5.3",
    "gatsby-source-contentful": "^2.3.14",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-remark": "^2.8.14",
    "gatsby-transformer-sharp": "^2.5.4",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.20.0",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "typeface-inter": "^3.12.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-react": "7.19.0",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "1.19.1",
    "tailwindcss": "1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobarriola/personal-site"
  },
  "bugs": {
    "url": "https://github.com/jacobarriola/personal-site/issues"
  }
}
