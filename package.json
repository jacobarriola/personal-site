    "gatsby-image": "^2.9.0",
    "gatsby-plugin-advanced-sitemap": "^1.5.6",
    "gatsby-plugin-catch-links": "^2.8.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-mdx": "^1.8.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-postcss": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.12.2",
    "gatsby-remark-autolink-headers": "^2.9.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^3.11.0",
    "gatsby-source-filesystem": "^2.9.1",
    "gatsby-transformer-remark": "^2.14.0",
    "gatsby-transformer-sharp": "^2.10.1",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.23.0",
    "prop-types": "15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "typeface-inter": "^3.15.1"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "^10.2.1",
    "babel-eslint": "10.1.0",
    "eslint": "^7.18.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "gatsby-plugin-eslint": "2.0.8",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "tailwindcss": "^1.9.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobarriola/personal-site"
  },
  "bugs": {
    "url": "https://github.com/jacobarriola/personal-site/issues"
  }
