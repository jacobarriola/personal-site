{
  "name": "jacob-arriola",
  "description": "Personal site for Jacob Arriola",
  "version": "1.0.0",
  "author": "Jacob Arriola",
  "dependencies": {
    "@reach/skip-nav": "^0.10.5",
    "gatsby": "^2.23.20",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-advanced-sitemap": "^1.5.5",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-offline": "^3.2.16",
    "gatsby-plugin-postcss": "^2.3.9",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-sharp": "^2.6.17",
    "gatsby-remark-autolink-headers": "^2.3.10",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^3.5.9",
    "gatsby-source-contentful": "^2.3.22",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-remark": "^2.8.23",
    "gatsby-transformer-sharp": "^2.5.10",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.20.0",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "typeface-inter": "^3.12.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "10.1.0",
    "eslint": "^7.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.3",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobarriola/personal-site"
  },
  "bugs": {
    "url": "https://github.com/jacobarriola/personal-site/issues"
  }
}
